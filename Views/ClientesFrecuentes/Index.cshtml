@model IEnumerable<prueba2maya.Models.ClientesFrecuente>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TotalCompras)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaUltimaCompra)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PuntosAcumulados)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdClienteNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TotalCompras)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaUltimaCompra)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PuntosAcumulados)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdClienteNavigation.IdUsuario)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdClienteFrecuente">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdClienteFrecuente">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdClienteFrecuente">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
